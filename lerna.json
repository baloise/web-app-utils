{
  "version": "3.4.0",
  "granularPathspec": false,
  "packages": [
    "packages/*"
  ],
  "command": {
    "version": {
      "message": "chore(release): publish %s"
    }
  },
  "ignoreChanges": [
    ".build/**",
    ".github/**",
    ".husky/**",
    "**/.build/**",
    "**/generated/**",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/*.e2e.ts",
    "**/*.md"
  ]
}
